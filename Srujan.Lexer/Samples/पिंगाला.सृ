अंक प्रवेश () 
{ 
	प्रथम: अंक = 0;
	दूसरा: अंक = 1;

	दिखाएँ "पिंगाला संख्याएँ";
	दिखाएँ प्रथम;
	दिखाएँ दूसरा;

	संख्यायें: अंक = 10;
	
	तीसरा: अंक = 0;
	गिनती: अंक  = 2;

	जबतक गिनती < संख्यायें
	{
		तीसरा = प्रथम + दूसरा;
		प्रथम = दूसरा;
		दूसरा = तीसरा;
		दिखाएँ तीसरा;
		गिनती = गिनती + 1;
	}


}