अंक प्रवेश () 
{ 
	एक: दशमलव  = 100.345;
	अगर एक > 50 तो
	{
		दिखाएँ एक;
	}

	विरा: अंक[3];
	विरा = {10, 20, 30};
	विरा[1] = 20;
	
	दि: अंक = 0;
	जबतक दि < 3
	{
	
		दिखाएँ "Current element: ";
		दिखाएँ विरा[दि];
		दि = दि + 1;
	}

	एक = 60;

	जबतक एक > 50
	{
		दिखाएँ एक;
		एक = एक - 1;
		अगर एक < 55 तो
		{
			दो: अंक = 30;
			जबतक दो < 50
			{
				दिखाएँ दो;
				दो = दो + 1;
				अगर एक < 55 तो
				{
					विराम;
				}
			}

		}
	}


	दिखाएँ "The answer is :";
	दिखाएँ एक;
}